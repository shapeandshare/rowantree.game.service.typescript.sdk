{
  "name": "rowantree.service.typescript.sdk",
  "version": "0.6.0",
  "description": "The Rowan Tree Service [TypeScript] SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shapeandshare/rowantree.service.typescript.sdk.git"
  },
  "author": "Joshua C. Burt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shapeandshare/rowantree.service.typescript.sdk/issues"
  },
  "homepage": "https://github.com/shapeandshare/rowantree.service.typescript.sdk#readme",
  "scripts": {
    "audit": "npm audit --registry=https://registry.npmjs.org/",
    "audit:fix": "npm audit fix --registry=https://registry.npmjs.org/",
    "build": "tsc",
    "pack": "npm run build && node build/pack.js",
    "clean": "rimraf dist .nyc_output coverage test-reports lint \"*.tgz\"",
    "lint": "eslint --ignore-path .gitignore --ext .ts --ext js .",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- -f json -o ./lint/lint_report.json",
    "prebuild": "npm run clean",
    "postbuild": "node build/postbuild.js",
    "start": "tsc --watch",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "nyc --reporter=lcov --reporter=text mocha \"test/unit/**/*.test.ts\"",
    "test:integration": "mocha \"test/integration/**/*.test.ts\""
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.3",
    "@types/eslint": "^8.4.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.17",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "rowantree.auth.typescript.sdk": "^0.14.0"
  }
}
